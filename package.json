{
  "name": "ember-css-modules",
  "version": "0.4.4",
  "description": "CSS Modules for ambitious applications",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "install-all": "for dummy in tests/dummy-*-addon; do (cd $dummy && npm install); done",
    "test": "npm run install-all && ember try:each"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/salsify/ember-css-modules.git"
  },
  "engines": {
    "node": ">= 0.12.0"
  },
  "author": "Dan Freeman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/salsify/ember-css-modules/issues"
  },
  "homepage": "https://github.com/salsify/ember-css-modules#readme",
  "devDependencies": {
    "ember-ajax": "^2.4.1",
    "ember-browserify": "^1.1.13",
    "ember-cli": "^2.8.0",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-htmlbars": "^1.0.8",
    "ember-cli-htmlbars-inline-precompile": "^0.3.2",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-qunit": "^3.0.1",
    "ember-cli-release": "^0.2.9",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-load-initializers": "^0.5.1",
    "ember-resolver": "^2.0.3",
    "loader.js": "^4.0.10"
  },
  "keywords": [
    "ember-addon",
    "css-modules"
  ],
  "dependencies": {
    "broccoli-concat": "^2.1.0",
    "broccoli-css-modules": "^0.4.2",
    "broccoli-funnel": "^1.0.7",
    "broccoli-merge-trees": "^1.1.1",
    "debug": "^2.2.0",
    "ember-cli-babel": "^5.1.5",
    "ember-getowner-polyfill": "^1.0.0",
    "ensure-posix-path": "^1.0.2",
    "hash-string": "^1.0.0",
    "postcss": "^5.2.2",
    "toposort": "^1.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "ember-cli-babel",
      "ember-cli-less",
      "ember-cli-sass",
      "ember-cli-stylus"
    ],
    "versionCompatibility": {
      "ember": ">=1.13.0 <3.0.0"
    },
    "paths": [
      "tests/dummy-addon",
      "tests/dummy-sass-addon",
      "tests/dummy-less-addon"
    ]
  }
}
