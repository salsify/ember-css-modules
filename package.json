{
  "name": "ember-css-modules",
  "version": "0.7.10",
  "description": "CSS Modules for ambitious applications",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "lint:js": "eslint .",
    "test-node": "qunit tests-node/{acceptance,unit}/**/*-test.js",
    "install-all": "cd tests/dummy-sass-addon && yarn && cd ../dummy-less-addon && yarn",
    "test": "yarn run lint-node && yarn run install-all && ember try:each",
    "test-win": "yarn run install-all && ember test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/salsify/ember-css-modules.git"
  },
  "engines": {
    "node": ">= 4"
  },
  "author": "Dan Freeman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/salsify/ember-css-modules/issues"
  },
  "homepage": "https://github.com/salsify/ember-css-modules#readme",
  "devDependencies": {
    "@ember-decorators/babel-transforms": "^2.0.1",
    "babel-eslint": "^8.2.5",
    "console-ui": "^2.2.2",
    "ember-ajax": "^3.0.0",
    "ember-browserify": "^1.2.1",
    "ember-cli": "^3.0.0",
    "ember-cli-addon-tests": "^0.11.0",
    "ember-cli-dependency-checker": "^2.1.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "^1.7.0",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^2.2.0",
    "ember-cli-uglify": "^2.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.5.0",
    "ember-sinon": "^1.0.1",
    "ember-source": "~3.0.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "eslint": "^4.18.1",
    "eslint-plugin-ember": "^5.0.3",
    "eslint-plugin-node": "^6.0.0",
    "loader.js": "^4.6.0",
    "postcss-color-rebeccapurple": "^3.0.0",
    "qunitjs": "^2.4.1",
    "sinon": "^4.3.0"
  },
  "keywords": [
    "ember-addon",
    "css-modules"
  ],
  "dependencies": {
    "@ember-decorators/utils": "^2.1.0",
    "broccoli-concat": "^3.2.2",
    "broccoli-css-modules": "^0.6.2",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^2.0.0",
    "broccoli-postcss": "^3.5.2",
    "calculate-cache-key-for-tree": "^1.1.0",
    "debug": "^3.1.0",
    "ember-cli-babel": "^6.11.0",
    "ember-cli-version-checker": "^2.1.0",
    "ensure-posix-path": "^1.0.2",
    "hash-string": "^1.0.0",
    "lodash.merge": "^4.6.1",
    "postcss": "^6.0.19",
    "semver": "^5.5.0",
    "toposort": "^1.0.6"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-font-awesome"
    ],
    "before": [
      "ember-cli-babel",
      "ember-cli-less",
      "ember-cli-sass",
      "ember-cli-stylus",
      "ember-cli-postcss"
    ],
    "paths": [
      "tests/dummy-addon",
      "tests/dummy-sass-addon",
      "tests/dummy-less-addon"
    ]
  }
}
