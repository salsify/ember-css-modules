{
  "name": "ember-css-modules",
  "version": "2.0.0",
  "description": "CSS Modules for ambitious applications",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember server",
    "test": "npm-run-all lint test:node test:ember",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:node": "qunit tests-node/**/*-test.js"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "bugs": {
    "url": "https://github.com/salsify/ember-css-modules/issues"
  },
  "homepage": "https://github.com/salsify/ember-css-modules#readme",
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.4.2",
    "@embroider/test-setup": "^0.43.5",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-plugin": "^1.3.1",
    "console-ui": "^2.2.2",
    "ember-auto-import": "^2.2.4",
    "ember-cli": "~3.28.4",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.3",
    "ember-source": "~3.28.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.6.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "postcss-color-rebeccapurple": "^6.0.0",
    "prettier": "^2.3.2",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "sinon": "^4.3.0",
    "template-stuff": "link:tests/dummy/lib/template-stuff",
    "webpack": "^5.64.1"
  },
  "keywords": [
    "ember-addon",
    "css-modules"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/salsify/ember-css-modules.git"
  },
  "license": "MIT",
  "author": "Dan Freeman",
  "dependencies": {
    "broccoli-bridge": "^1.0.0",
    "broccoli-concat": "^4.2.5",
    "broccoli-css-modules": "^0.8.0",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-postcss": "^6.0.0",
    "debug": "^4.3.2",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^6.0.0",
    "ensure-posix-path": "^1.0.2",
    "hash-string": "^1.0.0",
    "lodash.merge": "^4.6.1",
    "postcss": "^8.0.0",
    "toposort": "^2.0.2"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-font-awesome"
    ],
    "before": [
      "ember-cli-babel",
      "ember-cli-htmlbars",
      "ember-cli-less",
      "ember-cli-sass",
      "ember-cli-stylus",
      "ember-cli-postcss"
    ]
  },
  "volta": {
    "node": "16.13.0"
  }
}
